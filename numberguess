import random
number = random.randint(1, 100)

print("Type your guess between 0 and 100")

guess = -1
while guess !=number:
    
    guess = int(input("Your guess is:"))
        
    if guess == number:
        print("You guessed the number, congratulations)
    elif guess > number:
        print("A bit more down")
    else:
        print("A bit more up")
